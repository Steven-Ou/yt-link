{
  "name": "yt-link-frontend",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "homepage": "./",
  "scripts": {
    "dev": "rimraf .next && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "npm run build && electron-builder"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "electron-updater": "^6.2.1",
    "next": "14.2.4",
    "node-fetch": "^2.7.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "postcss": "^8",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.1",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.steven-ou.yt-link",
    "productName": "YT Link",
    "files": [
      "out/**/*",
      "main.js",
      "preload.js"
    ],
    "extraResources": [
      {
        "from": "../dist/yt-link-backend",
        "to": "backend/yt-link-backend"
      }
    ],
    "directories": {
      "buildResources": "build"
    },
    "mac": {
      "target": "dmg"
    }
  }
}
