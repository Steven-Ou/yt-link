{
  "name": "yt-link",
  "version": "0.0.1",
  "description": "YT Link allows you to download YouTube videos as MP3s, or entire playlists as a ZIP file.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "next dev",
    "build": "next build",
    "lint": "next lint",
    "package": "electron-builder"
  },
  "keywords": [],
  "author": "Steven Ou",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.16.4",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.2",
    "next": "^15.3.1",
    "node-fetch": "^2.7.0",
    "postcss": "^8",
    "react": "^18",
    "react-dom": "^18",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^2.2.19",
    "electron": "^31.2.1",
    "electron-builder": "^24.13.3",
    "electron-reloader": "^1.2.3",
    "eslint": "^8",
    "eslint-config-next": "15.3.1"
  },
  "build": {
    "appId": "com.steven-ou.yt-link",
    "productName": "YT Link",
    "directories": {
      "output": "dist"
    },
    "files": [
      "out/**/*",
      "main.js",
      "preload.js",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
