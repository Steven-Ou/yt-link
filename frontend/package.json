"build": {
  "appId": "com.steve.ytlink.final",
  "productName": "YT Link Final",
  "files": [
    "out/**/*",
    "main.js",
    "preload.js",
    "package.json"
  ],
  "win": {
    "target": "zip",
    "extraResources": [
      {
        "from": "../service/dist/app.exe",
        "to": "service/app.exe"
      }
    ]
  },
  "mac": {
    "target": "dmg",
    "extraResources": [
      {
        "from": "../service",
        "to": "service",
        "filter": [
          "**/*",
          "!dist/",
          "!build/"
        ]
      }
    ]
  },
  "publish": {
    "provider": "github",
    "owner": "Steven-Ou",
    "repo": "yt-link"
  }
},